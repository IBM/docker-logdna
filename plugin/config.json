    "documentation": "https://github.com/ibm/docker_logdna",
    "entrypoint": [
        "/docker_logdna"
    ],
    "network": {
        "type": "host"
    },
    "interface": {
        "types": [
            "docker.logdriver/1.0"
        ],
        "socket": "logdna.sock"
    }
}
